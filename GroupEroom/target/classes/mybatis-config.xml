<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "http://mybatis.org/dtd/mybatis-3-config.dtd" >
<configuration>
	<settings>
		<setting name="jdbcTypeForNull" value="NULL"/>
		<!-- 만약에 null로 데이터가 전달되었다면 빈칸이 아닌 NULL로 인식하도록 설정 -->
	</settings>
	
	<typeAliases>
		<typeAlias type="com.eroom.gw.approval.domain.Approval" alias="Approval" />
		<typeAlias type="com.eroom.gw.approval.domain.ApprovalFile" alias="ApprovalFile" />
		<typeAlias type="com.eroom.gw.approval.domain.ApprovalReply" alias="ApprovalReply" />
		<typeAlias type="com.eroom.gw.mail.domain.Mail" alias="Mail" />
		<typeAlias type="com.eroom.gw.mail.domain.MailFile" alias="MailFile" />
		<typeAlias type="com.eroom.gw.fboard.domain.Freeboard" alias="Freeboard" />
		<typeAlias type="com.eroom.gw.fboard.domain.FreeboardCmt" alias="FreeboardCmt" />
		<typeAlias type="com.eroom.gw.fboard.domain.FreeboardFile" alias="FreeboardFile" />
		<typeAlias type="com.eroom.gw.cboard.domain.CBoard" alias="cBoard"/>
		<typeAlias type="com.eroom.gw.cboard.domain.Reply" alias="Reply"/>
		<typeAlias type="com.eroom.gw.member.domain.Member" alias="Member"/>
		<typeAlias type="com.eroom.gw.attendance.domain.Attendance" alias="Attendance"/>
	</typeAliases>
	<mappers>
		<mapper resource="mappers/approval-mapper.xml"></mapper>
		<mapper resource="mappers/mail-mapper.xml"></mapper>
		<mapper resource="mappers/fBoard-mapper.xml"></mapper>
		<mapper resource="mappers/cBoard-mapper.xml"></mapper>
		<mapper resource="mappers/member-mapper.xml"></mapper>
		<mapper resource="mappers/attendance-mapper.xml"></mapper>
	</mappers>
	
</configuration>